      "providerPlugin": "awscloudformation"
    },
    "task": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    }
  },
  "function": {
    "categoryHandler": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "category",
          "attributes": [
            "Name",
            "Arn"
          ],
          "attributeEnvMap": {
            "Name": "TABLE_NAME",
            "Arn": "TABLE_ARN"
          }
        }
      ]
    },
    "taskHandler": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "task",
          "attributes": [
            "Name",
            "Arn"
          ],
          "attributeEnvMap": {
            "Name": "TABLE_NAME",
            "Arn": "TABLE_ARN"
          }
        }
      ]
    }
  },
  "auth": {
    "cognitodb243435": {
      "service": "Cognito",
      "serviceType": "managed",
      "providerPlugin": "awscloudformation"
    }
  },
  "api": {
    "category": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "categoryHandler",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    },
    "task": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
